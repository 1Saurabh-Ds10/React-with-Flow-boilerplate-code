{
  "name": "react-with-flow-boilerplate",
  "version": "1.0.0",
  "description": "Rect Boilerplate code with various configurations",
  "main": "index.js",
  "author": "Saurabh",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2  \"js/**/*.{js,jsx}\"",
    "clean": "rimraf public",
    "build": "npm run clean && set NODE_ENV=dev && webpack",
    "serve": "set NODE_ENV=dev && webpack-dev-server",
    "watch": "webpack --watch",
    "prod": "npm run clean && set NODE_ENV=prod && webpack -p",
    "build:dev": "npm run clean && webpack -d",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "snapshotSerializers": ["jest-serializer-enzyme"]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "prop-types": "^15.6.0",
